{
  "name": "clorio-client",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@ledgerhq/hw-transport-node-hid-singleton": "^5.46.0",
    "@ledgerhq/hw-transport-webhid": "^5.46.0",
    "@o1labs/client-sdk": "0.2.3",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-error": "1.1.13",
    "apollo-link-http": "1.5.17",
    "apollo-link-ws": "1.0.20",
    "apollo-utilities": "1.3.4",
    "big.js": "6.0.3",
    "bignumber.js": "9.0.1",
    "bootstrap": "4.6.0",
    "date-fns": "^2.19.0",
    "graphql": "15.5.0",
    "is-electron": "^2.2.0",
    "js-sha256": "0.9.0",
    "jspdf": "^2.3.1",
    "mina-ledger-js": "1.0.4",
    "nedb": "1.8.0",
    "nedb-promises": "^4.1.1",
    "node-sass": "4.14.1",
    "open": "^7.4.2",
    "react": "17.0.1",
    "react-bootstrap": "1.4.3",
    "react-cookie": "4.0.3",
    "react-dom": "17.0.1",
    "react-feather": "2.0.9",
    "react-router-dom": "5.1.2",
    "react-scripts": "4.0.1",
    "react-toastify": "^7.0.3",
    "react-tooltip": "^4.2.15",
    "subscriptions-transport-ws": "0.9.5"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "react-start": "BROWSER=none react-scripts start",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "postinstall": "install-app-deps",
    "package": "electron-builder build --mac --publish never",
    "electron-pack": "electron-builder -m"
  },
  "build": {
    "appId": "io.clor",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "*.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "portable"
    },
    "mac": {
      "icon": "./public/icons/mac/icon.icns",
      "category": "public.app-category.utilities"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "electron": "11.2.0",
    "electron-builder": "^22.9.1",
    "electron-is-dev": "^2.0.0",
    "electron-packager": "^15.2.0",
    "eslint": "7.18.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^5.1.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  }
}
