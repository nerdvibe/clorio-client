{
  "name": "clorio-client",
  "version": "0.1.1",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@apollo/client": "3.3.13",
    "@ledgerhq/hw-transport-node-hid-singleton": "5.46.0",
    "@ledgerhq/hw-transport-webhid": "5.46.0",
    "@o1labs/client-sdk": "0.2.5",
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-error": "1.1.13",
    "apollo-link-http": "1.5.17",
    "apollo-link-ws": "1.0.20",
    "apollo-utilities": "1.3.4",
    "append-query": "2.1.0",
    "base-x": "3.0.8",
    "big.js": "6.0.3",
    "bignumber.js": "9.0.1",
    "bootstrap": "4.6.0",
    "date-fns": "2.19.0",
    "graphql": "15.5.0",
    "is-electron": "2.2.0",
    "js-sha256": "0.9.0",
    "jspdf": "2.3.1",
    "mina-ledger-js": "1.0.6",
    "nedb": "1.8.0",
    "nedb-promises": "4.1.1",
    "node-sass": "5.0.0",
    "open": "7.4.2",
    "react": "17.0.1",
    "react-bootstrap": "1.4.3",
    "react-cookie": "4.0.3",
    "react-dom": "17.0.1",
    "react-feather": "2.0.9",
    "react-router-dom": "5.1.2",
    "react-scripts": "4.0.3",
    "react-toastify": "7.0.3",
    "react-tooltip": "4.2.15",
    "subscriptions-transport-ws": "0.9.5",
    "typescript": "4.2.3"
  },
  "scripts": {
    "start": "env-cmd -f ./.env.devnet nf start -p 3000",
    "start:mainnet": "env-cmd -f ./.env.mainnet nf start -p 3000",
    "build": "yarn build:devnet && yarn build:mainnet",
    "build:devnet": "yarn add @o1labs/client-sdk@0.2.5 && npm rebuild && env-cmd -f ./.env.devnet react-scripts build && rm -rf build-devnet && cp -r build build-devnet",
    "build:mainnet": "yarn add @o1labs/client-sdk@1.0.0 && npm rebuild && env-cmd -f ./.env.mainnet react-scripts build && rm -rf build-mainnet && cp -r build build-mainnet",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "react-start": "BROWSER=none react-scripts start",
    "pack:devnet": "env-cmd -f ./.env.devnet build --dir",
    "pack:mainnet": "env-cmd -f ./.env.mainnet build --dir",
    "postinstall": "install-app-deps",
    "package:devnet": "yarn build:devnet && electron-builder build -mwl --publish never && rm -rf dist-devnet && mv dist dist-devnet",
    "package:mainnet": "yarn build:mainnet && electron-builder build -mwl --publish never && rm -rf dist-mainnet && mv dist dist-mainnet",
    "package-only:mainnet": "electron-builder build -mwl --publish never && mv dist dist-mainnet"
  },
  "build": {
    "appId": "io.clor",
    "productName": "Clorio Wallet",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "*.js",
      "*.js",
      "*.ts",
      "*.tsx",
      "public/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "portable"
    },
    "mac": {
      "icon": "./public/icons/mac/icon.icns",
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "title": "Clorio Wallet",
      "background": "assets/background.png",
      "contents": [
        {
          "x": "320",
          "y": "280",
          "type": "file"
        },
        {
          "x": "580",
          "y": "280",
          "type": "link",
          "path": "/Applications"
        }
      ],
      "window": {
        "height": "450"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/big.js": "^6.0.2",
    "@types/react-router-dom": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.21.0",
    "babel-eslint": "10.1.0",
    "electron": "9.4.0",
    "electron-builder": "22.10.5",
    "electron-is-dev": "2.0.0",
    "electron-packager": "15.2.0",
    "env-cmd": "10.1.0",
    "eslint": "7.18.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "6.0.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "ts-node-dev": "1.0.0"
  },
  "resolutions": {
    "electron-builder/**/app-builder-bin": "3.5.13",
    "usb-detection": "4.10.0"
  }
}
